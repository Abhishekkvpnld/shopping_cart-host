
<section style="padding-top:60px" >
   <div id="carouselExampleControls" class="carousel slide" data-ride="carousel" style="background-color: aliceblue;width:100%;height:400px">
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img style="width: 100%;height: 400px;" class="d-block w-100" src="/images/62b5a9572dab880f81c5d178_ajVzMkY7zNN-cU8hLJlTXR93WXkC09AI_0Dm-VBCfWe-kbHdRAAATBpSlNajuRsW7e0jHYCOVjdcHY1Sf-3X4tAI22KAFbbu31rgYGEmgCSV_WUrLFWhWl09ddXm7EhIITjKG0Oggdx.png" alt="First slide">
    </div>
    <div class="carousel-item">
      <img style="width: 100%;height: 400px;" class="d-block w-100" src="/images/banner-1544x500.png" alt="Second slide">
    </div>
    <div class="carousel-item">
      <img style="width: 100%;height: 400px;" class="d-block w-100" src="/images/product-list-carousel-for-ecommerce-website (1).png" alt="third slide">
    </div>
     {{!-- <div class="carousel-item">
      <img style="width: 100%;height: 400px;" class="d-block w-100" src="/images/62b5a9572dab880f81c5d178_ajVzMkY7zNN-cU8hLJlTXR93WXkC09AI_0Dm-VBCfWe-kbHdRAAATBpSlNajuRsW7e0jHYCOVjdcHY1Sf-3X4tAI22KAFbbu31rgYGEmgCSV_WUrLFWhWl09ddXm7EhIITjKG0Oggdx.png" alt="forth slide">
    </div> --}}
  </div>
  <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>
  <div class="container">
    <div class="row">
      {{#each products}} <!-- Assuming 'products' is your array of product objects -->
      <div class="card col-6 col-md-3 mt-2 " style="width: 100px; height:350px; padding-top: 10px;border:1px solid;border-radius:15px;">
        <img class="card-img-top" style="width: 150px; height: 140px;" src="/product-image/{{this._id}}.png">
        <div class="card-body">
          <h5 class="card-title">{{productName}}</h5>
          <p class="card-text">Rs {{productPrice}}/-</p>
          <p class="card-text">{{this.productDetails}}</p>
         <button class="btn btn-primary" onclick="addToCart('{{this._id}}')" id="cart-count" value="">Add to Cart</button>
        </div>
      </div>
      {{/each}}
    </div>
  </div>
</section>
